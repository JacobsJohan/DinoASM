0000a0 2d25                      	MOV tempregister,randomNumber
                                 
0000a1 3023                      	CPI tempregister,3
0000a2 f408                      	BRSH noNewCactus // A 3/32 chance of new cactus
                                 
0000a3 dfa2                      	RCALL addCactus
                                 
