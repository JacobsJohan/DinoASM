                                 waitingLoop:
                                 // A loop that can be used to wait for a certain time (e.g. Latching time)
                                 // Changed registers: /
000021 932f                      	PUSH tempRegister
000022 930f                      	PUSH counter
                                 
000023 2722                      	CLR tempRegister
000024 2700                      	CLR counter
                                 	outerLoop:
000025 9523                      		INC tempRegister
000026 3f2f                      		CPI tempRegister,maxValueCounter0
000027 f7e9                      		BRNE outerLoop
                                 
                                 		middleLoop:
000028 2722                      			CLR tempRegister
000029 9503                      			INC counter
00002a 3003                      			CPI counter,maxValueCounter1
00002b f7c9                      			BRNE outerLoop	
                                 	finishLoop:
                                 
00002c 910f                      	POP counter
00002d 912f                      	POP tempRegister
00002e 9508                      RET
                                 
                                 collisionHandler:
                                 // Checks if collision has occured and handles the reaction on the collision
                                 // Changed registers: flags, gameState, buzzerCounter
                                 // Called functions: clearBuffer, drawGameOver, drawScore
00002f 932f                      	PUSH tempregister
                                 	
000030 ff30                      	SBRS flags,0
000031 c012                      	RJMP collisionRet
                                 
000032 7f3e                      	CBR flags,1 ; Clear the collision flag
000033 7f3d                      	CBR flags,0b00000010 ; Go back to normal mode from Extreme for game over screen
000034 7e3f                      	CBR flags,0b00010000 ; Go back to normal mode from Insane for game over screen
000035 d14b                      	RCALL clearBuffer	; Clear the screen such that not a new collision would happen (goal is to remove cacti)
000036 d242                      	RCALL drawGameOver
000037 e071                      	LDI gameState,1 ; Go to Game Over state
000038 d183                      	RCALL drawScore
000039 6034                      	SBR flags,0b00000100 ; Set the buzzersound flag
00003a 24cc                      	CLR buzzerCounter
                                 
                                 	; Re-use Timer 2
                                 
00003b e020                      	LDI tempRegister, 0x0	; Select normal counter mode
00003c 9320 00b0                 	STS TCCR2A, tempRegister
00003e e026                      	LDI tempRegister, 0x06	; Set timer2 prescaler to 256 
00003f 9320 00b1                 	STS TCCR2B,tempRegister
000041 eb29                      	LDI tempRegister, initialTimeValueBuzzer
000042 9320 00b2                 	STS TCNT2,tempRegister
                                 
                                 	collisionRet:
000044 912f                      	POP tempregister
000045 9508                      	RET
                                 
                                 addCactus:
                                 // Adds a new cactus to cactusMemory
                                 // Changed registers: nmbrOfCacti
000046 938f                      	PUSH xpos
000047 939f                      	PUSH ypos
000048 932f                      	PUSH tempregister
000049 93bf                      	PUSH XH
00004a 93af                      	PUSH XL
                                 
00004b e0b3                      	LDI XH, HIGH(cactusMemory)			// Point to start of cactusMemory
00004c e0a0                      	LDI XL, LOW(cactusMemory)
                                 
00004d 2d24                      	MOV tempregister, nmbrOfCacti
00004e 0f22                      	LSL tempregister
                                 
00004f 0fa2                      	ADD XL , tempregister
                                 
000050 e286                      	LDI xpos, 38							// xpos of cactus
000051 938d                      	ST X+, xpos
000052 e09a                      	LDI ypos, 10							// ypos of cactus
000053 939c                      	ST X, ypos
                                 
000054 9443                      	INC nmbrOfCacti
                                 
000055 91af                      	POP XL
000056 91bf                      	POP XH
000057 912f                      	POP tempregister
000058 919f                      	POP ypos
000059 918f                      	POP xpos
00005a 9508                      	RET
                                 
                                 moveCactus:
                                 // Changes the coordinates in Cactus memory. The coordinates are always moved as in normal mode, the difference between insane and normal mode is made in drawCacti
                                 // If the cactus is at the end, it deletes the cactus and puts a new one at starting position
                                 // Changed registers: nmbrOfCacti
                                 // Called functions: deleteFirstCactus
                                 
00005b 938f                      	PUSH xpos
00005c 932f                      	PUSH tempRegister
00005d 93bf                      	PUSH XH
00005e 93af                      	PUSH XL
                                 
00005f 2d24                      	mov tempregister, nmbrOfCacti
                                 
000060 e0b3                      	LDI XH, HIGH(cactusMemory) 
000061 e0a0                      	LDI XL, LOW(cactusMemory)
                                 
                                 	moveNextCactus:
000062 3020                      		CPI tempregister,0
000063 f059                      		BREQ noCactiLeft
000064 952a                      		DEC tempregister
                                 
000065 918c                      		LD xpos, X
000066 958a                      		DEC xpos			// Move cactus 1 xpos to the left
000067 f419                      		BRNE store
                                 			//Cactus is at the end left, must be deleted
000068 944a                      			DEC nmbrOfCacti
000069 d00a                      			RCALL deleteFirstCactus
00006a cff7                      			RJMP moveNextCactus
                                 
                                 		store:
00006b 938c                      			ST X, xpos
                                 
00006c 95a3                      			INC XL
00006d 95a3                      			INC XL
00006e cff3                      			RJMP moveNextCactus
                                 
                                 	noCactiLeft:
00006f 91af                      		POP XL
000070 91bf                      		POP XH
000071 912f                      		POP tempregister
000072 918f                      		POP xpos
000073 9508                      		RET
                                 
                                 deleteFirstCactus:
                                 // Deletes the first cactus of cactusMemory and shifts the remaining cacti
                                 // Changed registers: /
                                 // Called Functions: /
                                 
000074 938f                      	PUSH xpos
000075 932f                      	PUSH tempregister
000076 924f                      	PUSH nmbrOfCacti
000077 93bf                      	PUSH XH
000078 93af                      	PUSH XL
                                 
000079 2d24                      	mov tempregister, nmbrOfCacti
                                 
00007a e0b3                      	LDI XH, HIGH(cactusMemory) 
00007b e0a0                      	LDI XL, LOW(cactusMemory)
                                 
00007c 0f22                      	LSL tempregister
                                 
