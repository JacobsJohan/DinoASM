                                 // Changed registers: Z-pointer
                                 // Functions called: /
0000ca 932f                      	PUSH tempregister
                                 
0000cb e02f                      	LDI tempregister,0b00001111
0000cc 232e                      	AND tempregister,ZL
0000cd 3029                      	CPI tempregister,9
0000ce f410                      	BRSH overflowZLL
0000cf 95e3                      		INC ZL
0000d0 c019                      		RJMP updateScoreRet
                                 	overflowZLL:
0000d1 ef20                      		LDI tempregister,0b11110000
