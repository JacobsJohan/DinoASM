000094 2d24                      	MOV tempregister, nmbrOfCacti
000095 3020                      	CPI tempregister,0
000096 f041                      	BREQ noCheckNeeded				; If no Cactus on the screen, of course a new one is allowed.
000097 952a                      	DEC tempregister
000098 0f22                      	LSL tempregister
000099 0fa2                      	ADD XL , tempregister
00009a 918c                      	LD xpos, X
00009b 3280                      	CPI xpos,32						
00009c f438                      	BRSH noNewCactus				; To make sure that there is enough place between 2 cacti
                                 
00009d 3087                      	CPI xpos,7						; To make sure that at the moment of insane/normal mode transition there is enough place between 2 cacti
00009e f028                      	BRLO noNewCactus
                                 
