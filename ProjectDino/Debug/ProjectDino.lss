00007e 9612                      		ADIW XL,2
00007f 918c                      		LD xpos,X
000080 95aa                      		DEC XL
000081 95aa                      		DEC XL
000082 938d                      		ST X+,xpos
000083 952a                      		DEC tempregister
000084 cff8                      		RJMP stillDeleting
                                 
                                 
                                 	deletingDone:
000085 91af                      	POP XL
000086 91bf                      	POP XH
000087 904f                      	POP nmbrOfCacti
000088 912f                      	POP tempregister
000089 918f                      	POP xpos
                                 	
00008a 9508                      	RET
                                 
