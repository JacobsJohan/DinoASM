0000d4 f420                      		BRSH overflowZL
0000d5 2fe2                      			MOV ZL,tempregister
0000d6 e120                      			LDI tempregister,0b00010000
0000d7 0fe2                      			ADD ZL,tempregister
0000d8 c011                      			RJMP updateScoreRet
                                 		overflowZL:
0000d9 27ee                      			CLR ZL
0000da e02f                      			LDI tempregister,0b00001111
0000db 232f                      			AND tempregister,ZH
0000dc 3029                      			CPI tempregister,9
0000dd f410                      			BRSH overflowZHL
0000de 95f3                      				INC ZH
0000df c00a                      				RJMP updateScoreRet
                                 			overflowZHL:
0000e0 ef20                      				LDI tempregister,0b11110000
0000e1 232f                      				AND tempregister,ZH
